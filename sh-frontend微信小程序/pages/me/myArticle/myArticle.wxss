.good-title {
  font-size: 36rpx;
  margin: 10rpx 10rpx;

  overflow: hidden;
  /*超出一行文字自动隐藏*/
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* 商品列表 */
.good-box {

  padding: 20rpx 20rpx;
  border: 4rpx solid rgb(231, 231, 231);
}

.keyword_adddate {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 40rpx;
  justify-content: space-between;
}

.keyword {
  color: red;
  font-size: 30rpx;
  overflow: hidden;
  /*超出一行文字自动隐藏*/
  text-overflow: ellipsis;
  white-space: nowrap;

}
.time{
  color: rgb(170, 170, 170);
  width: 50%;
  overflow: hidden;
  /*超出一行文字自动隐藏*/
  text-overflow: ellipsis;
  white-space: nowrap;
}